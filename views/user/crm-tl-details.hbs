<script>
document.addEventListener('DOMContentLoaded', function () {
    // Add click event listener for 'Show Lead Status' buttons
    document.querySelectorAll('.show-lead-status-btn').forEach(function (button) {
        button.addEventListener('click', function () {
            const row = this.closest('tr');
            const leadStatusContent = row.querySelector('.leadStatusContent').textContent.trim();

            // Check if leadStatusContent exists and is not empty
            if (leadStatusContent) {
                try {
                    const leadStatus = JSON.parse(leadStatusContent); // Parse the JSON string

                    // Define the headings
                    const headings = ['QL', 'PQL', 'HQL', 'UQL', 'RNR', 'Busy', 'Call Later', 'Next Intake', 'QL to NR', 'PQL to NR', 'Not Interested', 'Just Enquiry', 'Regular', 'WhatsApp Connected', 'Direct University'];

                    // Build the HTML content to display the lead status details under headings
                    let leadStatusHtml = '<div class="container"><div class="row">';
                    headings.forEach((heading, index) => {
                        const statusDetails = leadStatus[heading] || { status: 'N/A', date: 'N/A' }; // Fallback if empty
                        leadStatusHtml += `
                            <div class="col-md-3 mb-3">
                                <div class="card">
                                    <div class="card-header bg-primary text-white">
                                        <h6 class="m-0 mhead">${heading}</h6>
                                    </div>
                                    <div class="card-body">
                                        <p class="m-0">Status: ${statusDetails.status || 'N/A'}</p>
                                        <p class="m-0">Date: ${statusDetails.date ? new Date(statusDetails.date).toLocaleString() : 'N/A'}</p>
                                    </div>
                                </div>
                            </div>`;
                        if ((index + 1) % 4 === 0) leadStatusHtml += '</div><div class="row">';
                    });
                    leadStatusHtml += '</div></div>';

                    // Insert lead status into modal
                    document.getElementById('leadStatusContent').innerHTML = leadStatusHtml;

                    // Show the modal
                    const leadStatusModal = new bootstrap.Modal(document.getElementById('leadStatusModal'));
                    leadStatusModal.show();
                } catch (error) {
                    console.error('Invalid JSON or error parsing lead status content:', error);
                }
            } else {
                console.error('No lead status content available.');
            }
        });
    });
});
</script>

<script>

async function toggleAssignment(button) {
    const row = button.closest('tr');
    const idElement = row.querySelector('.font-weight-bold');
    const id = idElement.getAttribute('data-id').trim();
    const source = idElement.getAttribute('data-source').trim();
    const leadOwnerSelect = row.querySelector('select');
    const leadOwnerName = leadOwnerSelect.value;
    const assignLead = button.classList.contains('btn-paid') ? null : 'assigned';
    const leadStatusTextarea = row.querySelector('.lead-status-textarea');
    const leadStatus = leadStatusTextarea ? leadStatusTextarea.value : '';

    if (!id || !source) {
        console.error('Invalid or missing ID or source');
        Swal.fire({
            icon: 'error',
            title: 'Error',
            text: 'Invalid or missing ID or source.',
        });
        return;
    }

    const assignDate = assignLead ? new Date().toISOString() : null;

    try {
        const response = await fetch('/update-lead-owner', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify({ id, source, leadOwnerName, assignLead, leadStatus, assignDate }), // Send assignDate
        });

        const result = await response.json();

        if (result.success) {
            Swal.fire({
                icon: 'success',
                title: 'Updated',
                text: 'Lead owner has been updated successfully!',
            }).then(() => {
                button.classList.toggle('btn-paid');
                button.classList.toggle('btn-not-paid');
                button.textContent = assignLead ? 'Assigned to lead owner' : 'Assign to lead owner';
            });
        } else {
            Swal.fire({
                icon: 'error',
                title: 'Error',
                text: result.message || 'An error occurred while updating the lead owner.',
            });
        }
    } catch (error) {
        console.error('Error:', error);
        Swal.fire({
            icon: 'error',
            title: 'Error',
            text: 'An error occurred while updating the lead owner.',
        });
    }
}




</script>

