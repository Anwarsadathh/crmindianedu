<script>
document.addEventListener('DOMContentLoaded', function () {
    // Add click event listener for 'Show Lead Status' buttons
    document.querySelectorAll('.show-lead-status-btn').forEach(function (button) {
        button.addEventListener('click', function () {
            const row = this.closest('tr');
            const leadStatusContent = row.querySelector('.leadStatusContent').textContent.trim();

            // Check if leadStatusContent exists and is not empty
            if (leadStatusContent) {
                try {
                    const leadStatus = JSON.parse(leadStatusContent); // Parse the JSON string

                    // Define the headings
                    const headings = ['QL', 'PQL', 'HQL', 'UQL', 'RNR', 'Busy', 'Call Later', 'Next Intake', 'QL to NR', 'PQL to NR', 'Not Interested', 'Just Enquiry', 'Regular', 'WhatsApp Connected', 'Direct University'];

                    // Build the HTML content to display the lead status details under headings
                    let leadStatusHtml = '<div class="container"><div class="row">';
                    headings.forEach((heading, index) => {
                        const statusDetails = leadStatus[heading] || { status: 'N/A', date: 'N/A' }; // Fallback if empty
                        leadStatusHtml += `
                            <div class="col-md-3 mb-3">
                                <div class="card">
                                    <div class="card-header bg-primary text-white">
                                        <h6 class="m-0 mhead">${heading}</h6>
                                    </div>
                                    <div class="card-body">
                                        <p class="m-0">Status: ${statusDetails.status || 'N/A'}</p>
                                        <p class="m-0">Date: ${statusDetails.date ? new Date(statusDetails.date).toLocaleString() : 'N/A'}</p>
                                    </div>
                                </div>
                            </div>`;
                        if ((index + 1) % 4 === 0) leadStatusHtml += '</div><div class="row">';
                    });
                    leadStatusHtml += '</div></div>';

                    // Insert lead status into modal
                    document.getElementById('leadStatusContent').innerHTML = leadStatusHtml;

                    // Show the modal
                    const leadStatusModal = new bootstrap.Modal(document.getElementById('leadStatusModal'));
                    leadStatusModal.show();
                } catch (error) {
                    console.error('Invalid JSON or error parsing lead status content:', error);
                }
            } else {
                console.error('No lead status content available.');
            }
        });
    });
});
</script>
<script>

function redirectToSEForm(button) {
    const row = button.closest('tr');
     const name = row.querySelector('td:nth-child(6)').textContent.trim();
    const email = row.querySelector('td:nth-child(18)').textContent.trim();
    const mobile = row.querySelector('td:nth-child(8)').textContent.trim();
    const course = row.querySelector('td:nth-child(15)').textContent.trim();
    const specialization = row.querySelector('td:nth-child(26)').textContent.trim();
    const city = row.querySelector('td:nth-child(14)').textContent.trim();
    const state = row.querySelector('td:nth-child(5)').textContent.trim();

    // Construct URL with query parameters
    const url = new URL('/se-form', window.location.origin);
    url.searchParams.append('name', encodeURIComponent(name));
    url.searchParams.append('email', encodeURIComponent(email));
    url.searchParams.append('mobile', encodeURIComponent(mobile));
    url.searchParams.append('course', encodeURIComponent(course));
    url.searchParams.append('specialization', encodeURIComponent(specialization));
    url.searchParams.append('city', encodeURIComponent(city)); // Add city
    url.searchParams.append('state', encodeURIComponent(state)); // Add state

    // Redirect to SE Form page
    window.location.href = url.toString();
}
</script>

<script>
async function updateLeadStatus(select) {
    const row = select.closest('tr');
    const leadStatusTextarea = row.querySelector('.lead-status-textarea');
    const leadStage = select.value;
    const id = row.querySelector('.font-weight-bold')?.dataset.id; // Ensure `id` is fetched safely

    // Ensure elements exist before proceeding
    if (!leadStatusTextarea) {
        console.error('Textarea not found in the row.');
        return;
    }

    if (leadStage) {
        try {
            const response = await fetch('/get-lead-status', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ id, leadStage })
            });

            const result = await response.json();
            console.log('Fetch result:', result);

            if (result.success) {
                if (result.status && result.status.trim() !== '' && result.status !== 'No Status') {
                    leadStatusTextarea.value = result.status;
                    leadStatusTextarea.disabled = true; // Disable textarea if status is found
                } else {
                    leadStatusTextarea.value = 'No Status';
                    leadStatusTextarea.disabled = false; // Enable textarea if no valid status is found
                }
            } else {
                console.log('Fetch failed:', result.message);
                leadStatusTextarea.value = 'No Status';
                leadStatusTextarea.disabled = false; // Enable textarea on failure
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: result.message || 'Failed to fetch lead status.',
                });
            }
        } catch (error) {
            console.error('Error:', error);
            leadStatusTextarea.value = 'No Status';
            leadStatusTextarea.disabled = false; // Enable textarea if error occurs
            Swal.fire({
                icon: 'error',
                title: 'Error',
                text: 'An error occurred while fetching the lead status.',
            });
        }
    } else {
        leadStatusTextarea.value = 'No Status';
        leadStatusTextarea.disabled = false; // Enable textarea if no lead stage is selected
    }
}



async function saveLeadStatus(button) {
    const row = button.closest('tr');
    const id = row.querySelector('.font-weight-bold').textContent.trim();
    const leadStageSelect = row.querySelector('.lead-stage-select');
    const leadStatusTextarea = row.querySelector('.lead-status-textarea');
    const leadStage = leadStageSelect.value;
    const leadStatus = leadStatusTextarea.value;
    const isSaved = leadStatus ? "saved" : null;

    console.log("ID:", id); // Debugging output
    console.log("Lead Stage:", leadStage); // Debugging output
    console.log("Lead Status:", leadStatus); // Debugging output
    console.log("Is Saved:", isSaved); // Debugging output

    try {
        const response = await fetch('/update-lead-status', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ id, leadStage, leadStatus, isSaved })
        });

        const result = await response.json();

        if (result.success) {
            Swal.fire({
                icon: 'success',
                title: 'Saved',
                text: 'Lead status has been updated successfully!',
            });
        } else {
            Swal.fire({
                icon: 'error',
                title: 'Error',
                text: result.message || 'An error occurred while updating the lead status.',
            });
        }
    } catch (error) {
        console.error('Error:', error);
        Swal.fire({
            icon: 'error',
            title: 'Error',
            text: 'An error occurred while updating the lead status.',
        });
    }
}


